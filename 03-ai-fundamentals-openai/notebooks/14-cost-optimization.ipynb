      "outputs": [],
      "source": [
        "# Cost calculation\ndef calculate_cost(prompt_tokens, completion_tokens, model='gpt-3.5-turbo'):\n    pricing = {'gpt-3.5-turbo': {'input': 0.0015, 'output': 0.002}}\n    rates = pricing[model]\n    return (prompt_tokens/1000 * rates['input']) + (completion_tokens/1000 * rates['output'])\n\nprint(f'Cost for 1000/2000 tokens: ${calculate_cost(1000, 2000):.4f}')"
      ]
    }
  ]
}