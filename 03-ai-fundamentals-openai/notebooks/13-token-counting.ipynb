      "source": [
        "import tiktoken\n\nencoding = tiktoken.encoding_for_model('gpt-3.5-turbo')\ntext = 'Hello world!'\ntokens = encoding.encode(text)\nprint(f'Text: {text}')\nprint(f'Tokens: {tokens}')\nprint(f'Count: {len(tokens)}')"
      ]
